<h1>Hooks#use</h1>
<%= form_tag("/hooks/saveusage", :method => "GET") do %>
<%= text_area_tag :message , @myfile, :size => "100x40" %>
<%= text_area_tag :hook_name %>
<%= select_tag :repositories, options_from_collection_for_select(Repository.all, "id", "repo_name") %>
<br/>
<%= submit_tag("save") %>
<% end %>